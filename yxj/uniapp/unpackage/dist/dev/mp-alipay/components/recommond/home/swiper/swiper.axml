<block a:if="{{swiperList}}"><swiper class="swiper data-v-5f6800ea" indicator-dots="{{false}}" circular="{{true}}" autoplay="{{false}}" interval="5000" duration="500" indicator-color="#8799axx3" indicator-active-color="#fff" data-event-opts="{{[['change',[['cardSwiper',['$event']]]]]}}" onChange="__e"><block a:for="{{swiperList}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item class="{{(('data-v-5f6800ea')+' '+(cardCur==index?'cur':''))}}"><view class="swiper-item data-v-5f6800ea"><image src="{{item.subjectImg}}" mode="aspectFill" class="data-v-5f6800ea"></image><view class="slide-mask data-v-5f6800ea"><text class="liveIcon data-v-5f6800ea">直播</text><text class="livePv data-v-5f6800ea">{{item.pvNum+"人在看"}}</text><text class="liveTitle data-v-5f6800ea">{{item.subjectTitle}}</text></view></view></swiper-item></block></swiper></block>